@model List<SiteSocialMediaResultDto>

<style>
    .footericon i
    {
        color: #000!important;
    }
</style>

@foreach (var item in Model)
{
    <a class="btn btn-square btn-secondary rounded-circle me-2 footericon" target="_blank" href="@item.SiteSocialMediaLink">@Html.Raw(item.SiteSocialMediaIcon)</a>
}