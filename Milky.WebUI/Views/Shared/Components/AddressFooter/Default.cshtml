@model List<AddressResultDto>

@if (Model.Count() > 0)
{
    <p class="mb-2"><i class="fa fa-map-marker-alt me-3"></i>@Model[0].AddressContent</p>
    <p class="mb-2"><i class="fa fa-phone-alt me-3"></i>@Model[0].AddressPhone</p>
    <p class="mb-2"><i class="fa fa-envelope me-3"></i>@Model[0].AddressMail</p>
}